@model PacotesTuristicos
@{
    ViewData["Title"] = "Cadastro de Viagens";
}
<h2>Cadastro Viagens</h2>
<form asp-action="CadastrarViagem" method="POST">
    <p>
        <label>Nome</label>
        <input asp-for="Nome">
    </p>
    <p>
        <label>Origem</label>
        <input asp-for="Origem">
    </p>
    <p>
        <label>Destino/label>
        <input asp-for="Destino">
    </p>
    <p>
        <label>Atrativos</label>
        <input asp-for="Atrativos">
    </p>
    <p>
        <label>Saida</label>
        <input asp-for="Saida">
    </p>
    <p>
        <label>Retorno</label>
        <input asp-for="Retorno">
    </p>
    <p>
        <input type="submit" value="Cadastrar">
    </p>
</form>